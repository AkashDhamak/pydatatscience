first_num = int(input("Start of Multiplication table is : "))
n = int(input("Stop on reaching : "))
while first_num < n+1 :
  second_num = 1
  while second_num < 11 :
    multiple = first_num * second_num 
    print(first_num,"X",second_num,"=" , multiple)
    second_num = second_num + 1
  print()
  first_num = first_num + 1
